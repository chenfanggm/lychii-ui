{
  "name": "lychii-ui",
  "version": "0.0.20",
  "description": "The UI interface for project LYCHII",
  "main": "/dist/index.js",
  "repository": "https://github.com/chenfanggm/lychii-ui.git",
  "keywords": [
    "Lychii",
    "ReactBot",
    "CLI",
    "React"
  ],
  "author": "Chen Fang",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.1.0",
    "npm": "^5.3.0"
  },
  "bugs": {
    "url": "https://github.com/chenfanggm/lychii-ui/issues"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "better-npm-run dev",
    "start": "better-npm-run start",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy:prod",
    "deploy:dev": "better-npm-run deploy:dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "lint": "eslint src tests",
    "lint:fix": "npm run lint -- --fix"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon --exec babel-node bin/start",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/start",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start build/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "better-npm-run": "^0.1.0",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "lodash-es": "^4.17.4",
    "moment": "^2.18.1",
    "normalize.css": "^7.0.0",
    "path": "^0.12.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^1.3.1",
    "redbox-react": "^1.5.0",
    "socket.io": "^2.0.3",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-import": "^1.3.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.25.0",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "chai-enzyme": "^0.8.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "enzyme": "^2.8.2",
    "eslint": "^4.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "font-awesome-sass-loader": "^2.0.1",
    "fs": "0.0.2",
    "fs-extra": "^4.0.1",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "isparta-loader": "^2.0.0",
    "karma": "^1.7.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.6.0",
    "resolve-url-loader": "^2.0.3",
    "sass-loader": "^6.0.6",
    "sinon": "^3.0.0",
    "sinon-chai": "^2.11.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.0",
    "yargs": "^8.0.2"
  }
}
